{
  "name": "@manufactura/infrastructure",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:migrate:create": "prisma migrate dev --create-only --name baseline --schema prisma/schema.prisma",
    "prisma:migrate:rollback": "prisma migrate resolve --rolled-back",
    "prisma:migrate:dev": "prisma migrate dev --schema prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@manufactura/shared": "workspace:*",
    "@prisma/client": "^6.3.1",
    "@supabase/supabase-js": "^2.48.1",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "prisma": "^6.3.1"
  }
}
