generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("SUPABASE_DATABASE_TRANSACTION_POOL_URL")
  directUrl = env("SUPABASE_DATABASE_URL")
}

model Healthcheck {
  id        String   @id @default(cuid())
  message   String
  createdAt DateTime @default(now()) @map("created_at")

  @@map("healthcheck")
}
